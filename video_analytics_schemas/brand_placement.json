{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Brand and Logo Placement",
  "type": "object",
  "properties": {
    "placements": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "timestamp": {
            "type": "string"
          },
          "brand": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "context": {
            "type": "string"
          },
          "logo_detected": {
            "type": "boolean"
          }
        },
        "required": [
          "timestamp",
          "brand",
          "type"
        ]
      }
    }
  }
}